    hauntbool = false
end

-- ;blackhole
local function blackhole(targetPlayer)
  if not targetPlayer then return nil end
  
  if blackholeConnection then
      blackholeConnection:Disconnect()
      blackholeConnection = nil
  end

  blackholebool = true
  
  local character = player.Character or player.CharacterAdded:Wait()
  local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
  local humanoid = character:WaitForChild("Humanoid")
  local head = character:WaitForChild("Head")
  
  local followDistance = Vector3.new(-0.5, -0.5, -0.5)
  local teleportDistance = Vector3.new(-0.5, -0.5, -0.5)
  
  local repulsionRadius = 5.5
  local repulsionForce = 5
  
  local leaderRootPart = nil
  
  local positionVelocity = Vector3.new(0, 0, 0)
  local springVelocity = Vector3.new(0, 0, 0)

  local stiffness = Vector3.new(3, 3, 3)
  local damping = Vector3.new(0.8, 0.8, 0.8)
  local jumpBoost = 5
  local bounceFactor = 0.8
  local raycastLength = 1.5

  local originalGravity = game.Workspace.Gravity
  game.Workspace.Gravity = 0

  if humanoid then
    humanoid:ChangeState(Enum.HumanoidStateType.Physics)
  end
  
  local function findRoot()
      if targetPlayer and targetPlayer.Character then
          leaderRootPart = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
          return leaderRootPart ~= nil
      end
      return false
  end
  
  if not findRoot() then 
      blackholebool = false
      return nil
  end

  local leaderCFrame = leaderRootPart.CFrame
  local teleportPosition = leaderCFrame.Position - 
      (leaderCFrame.RightVector * teleportDistance.X + 
       leaderCFrame.UpVector * teleportDistance.Y + 
       leaderCFrame.LookVector * teleportDistance.Z)
  humanoidRootPart.CFrame = CFrame.new(teleportPosition, leaderRootPart.Position)
  
  local lastLeaderPosition = leaderRootPart.Position
  
  local function isTouchingGround()
      local origin = humanoidRootPart.Position
      local direction = Vector3.new(0, -raycastLength, 0)
      local params = RaycastParams.new()
      params.FilterDescendantsInstances = {character}
      params.FilterType = Enum.RaycastFilterType.Blacklist
      
      local result = game.Workspace:Raycast(origin, direction, params)
      return result ~= nil
  end

  local function checkBotRepulsion()
      local nearbyParts = game.Workspace:GetPartBoundsInRadius(humanoidRootPart.Position, repulsionRadius)
      local repulsionVector = Vector3.new(0, 0, 0)
      
      for _, part in ipairs(nearbyParts) do
          local nearbyCharacter = part:FindFirstAncestorOfClass("Model")
          if nearbyCharacter and nearbyCharacter ~= character then
              local nearbyRootPart = nearbyCharacter:FindFirstChild("HumanoidRootPart")
              if nearbyRootPart then
                  local repulsionDirection = (humanoidRootPart.Position - nearbyRootPart.Position).Unit
                  local distance = (humanoidRootPart.Position - nearbyRootPart.Position).Magnitude

                  local inverseFalloff = math.max(0, 1 - math.exp(-distance/repulsionRadius))
                  repulsionVector = repulsionVector + (repulsionDirection * repulsionForce * inverseFalloff * 0.5)
              end
          end
      end
      
      return repulsionVector
  end
  
  blackholeConnection = game:GetService("RunService").Heartbeat:Connect(function(dt)
      if not blackholebool or not player or not player.Character then
          if blackholeConnection then
              blackholeConnection:Disconnect()
              blackholeConnection = nil
          end
          game.Workspace.Gravity = originalGravity
          if humanoid then humanoid.Sit = false end
          return
      end
      
      if not findRoot() then
          blackholebool = false
          if blackholeConnection then
              blackholeConnection:Disconnect()
              blackholeConnection = nil
          end
          game.Workspace.Gravity = originalGravity
          humanoid.Sit = false
          return
      end
      
      local leaderCFrame = leaderRootPart.CFrame
      local targetPosition = leaderCFrame.Position - 
          (leaderCFrame.RightVector * followDistance.X + 
           leaderCFrame.UpVector * followDistance.Y + 
           leaderCFrame.LookVector * followDistance.Z)
      
      local displacement = targetPosition - humanoidRootPart.Position
      
      local acceleration = Vector3.new(
          displacement.X * stiffness.X - positionVelocity.X * damping.X,
          displacement.Y * stiffness.Y - positionVelocity.Y * damping.Y,
          displacement.Z * stiffness.Z - positionVelocity.Z * damping.Z
      )

      local positionDelta = leaderRootPart.Position - lastLeaderPosition
      if math.abs(positionDelta.Y) > 1.5 then
          springVelocity = springVelocity + Vector3.new(0, jumpBoost * 1.5, 0)
      end
      lastLeaderPosition = leaderRootPart.Position

      local repulsionVector = checkBotRepulsion()
      positionVelocity = positionVelocity + acceleration * dt + repulsionVector * dt

      if isTouchingGround() then
          if positionVelocity.Y < 0 then
              positionVelocity = Vector3.new(
                  positionVelocity.X, 
                  -positionVelocity.Y * bounceFactor, 
                  positionVelocity.Z
              )
          end
      end

      local newPosition = humanoidRootPart.Position + positionVelocity * dt
      
      humanoidRootPart.CFrame = CFrame.new(newPosition, leaderRootPart.Position)
      head.CFrame = CFrame.Lerp(head.CFrame, leaderRootPart.CFrame, 0.2)
  end)
  return blackholeConnection
end

